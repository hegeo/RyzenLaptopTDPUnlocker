            </Grid>
            <Grid HorizontalAlignment="Left" Height="44" Margin="7,159,0,0" VerticalAlignment="Top" Width="776" AutomationProperties.HelpText="触发温度限制后的功耗墙" Style="{DynamicResource GridStyleHLC}">
